import { AfterViewInit, EventEmitter, OnInit } from '@angular/core';
import { BarChartOption } from '../../_model/barChartOptions';
import { ChartData, ChartView } from '../../_model/chartData';
export declare class BarChartComponent implements OnInit, AfterViewInit {
    selectedItem: EventEmitter<string>;
    chartData: ChartData[];
    chartOptions: BarChartOption;
    view: ChartView;
    lines: any[];
    totalSum: number;
    isHorizontal: boolean;
    barLineType: string;
    chartView: any[];
    scale: number;
    constructor();
    ngOnInit(): void;
    ngAfterViewInit(): void;
    getItemClicked(line: any): void;
    calculateHorizontalBarLines(graphData: any): any[];
    calculateVerticalBarLines(graphData: any): any[];
    getTotalSum(chartData: any): void;
}
