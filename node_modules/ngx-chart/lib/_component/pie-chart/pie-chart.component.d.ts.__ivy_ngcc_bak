import { ElementRef, EventEmitter, OnInit, QueryList } from '@angular/core';
import { ChartData, ChartOption } from '../../_model/chartData';
import { PieChartView } from '../../_model/pieChartOptions';
export declare class PieChartComponent implements OnInit {
    chartData: ChartData[];
    span: QueryList<ElementRef>;
    selectedItem: EventEmitter<any>;
    chartOptions: ChartOption;
    view: PieChartView;
    chartView: any[];
    totalSum: number;
    processedData: any[];
    legendData: any[];
    constructor();
    ngOnInit(): void;
    ngAfterViewInit(): void;
    polarToCartesian(centerX: any, centerY: any, radius: any, angleInDegrees: any): {
        x: any;
        y: any;
    };
    describeArc(x: any, y: any, radius: any, startAngle: any, endAngle: any): string;
    getPercentage(partialValue: any, totalValue: any): number;
    getItemClicked(item: any): void;
}
