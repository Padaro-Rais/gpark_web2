import { EventEmitter } from '@angular/core';
import { BaseCdsDirective } from '../../cds-base';
export declare class CdsSearchDirective extends BaseCdsDirective {
    get status(): any;
    set status(value: any);
    get controlWidth(): any;
    set controlWidth(value: any);
    get validate(): any;
    set validate(value: any);
    get responsive(): any;
    set responsive(value: any);
    get layout(): any;
    set layout(value: any);
    get layoutStable(): any;
    set layoutStable(value: any);
    layoutChange: EventEmitter<CustomEvent>;
}
