import { BaseCdsDirective } from '../../cds-base';
export declare class CdsInternalControlGroupDirective extends BaseCdsDirective {
    get status(): any;
    set status(value: any);
    get layout(): any;
    set layout(value: any);
    get controlAlign(): any;
    set controlAlign(value: any);
    get disabled(): any;
    set disabled(value: any);
    get controlWidth(): any;
    set controlWidth(value: any);
    get responsive(): any;
    set responsive(value: any);
    get layoutStable(): any;
    set layoutStable(value: any);
}
